{
  "name": "ijump",
  "displayName": "IJump",
  "description": "Go语言接口与实现智能跳转工具，支持跨文件分析",
  "version": "0.1.1",
  "publisher": "zhengxiaojun",
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "resources/icon.png",
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ijump.jumpToImplementation",
        "title": "Jump to Implementation"
      },
      {
        "command": "ijump.jumpToInterface",
        "title": "Jump to Interface Definition"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ijump.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "keywords": [
    "go",
    "golang",
    "interface",
    "navigation",
    "productivity",
    "code-navigation"
  ]
}
